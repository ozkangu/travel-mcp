# Server
PORT=3000
HOST=0.0.0.0

# Map Provider: osm (default) or mapbox
MAP_PROVIDER=osm

# Mapbox token (required if MAP_PROVIDER=mapbox)
# MAPBOX_TOKEN=your_mapbox_token_here

# Geocoding Provider: nominatim (default) or mapbox
GEOCODING_PROVIDER=nominatim

# Cache settings
CACHE_ENABLED=false
CACHE_TTL=300

# Rate limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# Output directory for temporary map images
OUTPUT_DIR=/tmp/mcp-maps
